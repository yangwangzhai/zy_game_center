/* common */
* { word-wrap: break-word; outline: none; }
body { background: #FFF; }
body, td, input, textarea, select, button { color: #333; font: 12px "Lucida Grande", Verdana, Lucida, Helvetica, Arial, 'Simsun',  sans-serif; font-size:12px;}
ul, li,ol { list-style: none; }
body, ul, dl, dd, p, h1, h2, h3, h4, h5, h6, form, fieldset { margin: 0; padding: 0; }
h1, h2, h3, h4, h5, h6 { font-size: 12px; }
a { color:#066cbc; text-decoration: none; }
a:hover { text-decoration: underline; color:#ff6600 }
a img { border: none; }
em, cite, th { font-style: normal; font-weight: normal; }
table { border-collapse: collapse; }
input[type=submit], input[type=reset], input[type=button]{ filter:chroma(color=#000000); }

/* form */
textarea, input { padding:3px 5px; border:#ccc 1px solid; border-radius:3px; line-height:18px; }
input[type=text]{box-shadow: inset 0 3px 3px #ddd;}
select{padding: 5px;}
input.txt,input.txt:hover, input.txt:focus, textarea:hover, textarea:focus { border-color: #5daaea; background: #F5F9FD; }
