.btn {margin: 3px 0; height:25px; line-height:20px; background:url(okbtn.png) repeat-x; color: #fff; cursor: pointer; padding:0 8px; border:#0772cb 1px solid; border-radius:3px; vertical-align:top; margin:-1px 0 0 0; /*min-width:100px; font-weight:bold; */display:inline-block; font-size:12px;}
.btn2 {margin: 3px 0; height:30px; line-height:28px; background:url(okbtn_home.png) repeat-x; color: #fff; cursor: pointer; padding:0 8px; border:#219808 1px solid; border-radius:3px;  }
